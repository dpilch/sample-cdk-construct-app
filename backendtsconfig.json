{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "lib": [
        "es2020"
      ],
      "declaration": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": false,
      "inlineSourceMap": true,
      "inlineSources": true,
      "experimentalDecorators": true,
      "strictPropertyInitialization": false,
      "typeRoots": [
        "./node_modules/@types"
      ],
      "rootDir": "src",
      "outDir": "lib",
      "skipLibCheck": true
    },
    "exclude": [
      "node_modules",
      "cdk.out"
    ]
  }
  